{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/no-unresolved": 0,
        "import/extensions": 0,
        "no-console": 0,
        "no-use-before-define": ["error", { "functions": false }],
        "no-unused-vars": ["error", { "args": false }]
    }
}
