// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TeamMember {
  id            String  @id @default(uuid())
  // Identity
  firstName     String
  lastName      String
  // Job
  title         String
  startYear     Int
  wasApprentice Boolean
  // Personal
  pronouns      String?
  jobLikes      String
  interests     String
  // Socials
  linkedin      String?
  github        String?
  youtube       String?
  personal      String?
  instagram     String?
}
